{
  "version": 2,
  "name": "household-services-frontend",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "https://your-backend.up.railway.app"
  },
  "build": {
    "env": {
      "VITE_API_URL": "https://your-backend.up.railway.app"
    }
  }
}