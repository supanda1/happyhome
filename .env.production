# Production Environment Configuration
# This file contains production-specific environment variables

# App Configuration
VITE_APP_NAME=Household Services
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Professional household services booking platform

# Environment
NODE_ENV=production
VITE_NODE_ENV=production

# API Configuration - Update with your production API URL
VITE_API_URL=https://api.yourdomain.com/api
VITE_API_TIMEOUT=15000

# AWS Configuration - Update with your AWS resources
VITE_AWS_REGION=us-east-1
VITE_AWS_S3_BUCKET=household-services-prod-uploads
VITE_AWS_CLOUDFRONT_URL=https://your-cloudfront-url.cloudfront.net

# Google Maps (if using)
VITE_GOOGLE_MAPS_API_KEY=your-production-google-maps-api-key

# Analytics - Update with your production IDs
VITE_GA_TRACKING_ID=G-XXXXXXXXXX
VITE_HOTJAR_ID=your-production-hotjar-id

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_ENABLE_MAINTENANCE_MODE=false

# Security
VITE_ENABLE_CSP=true
VITE_ALLOWED_ORIGINS=https://yourdomain.com,https://www.yourdomain.com

# Performance
VITE_ENABLE_PWA=true
VITE_ENABLE_OFFLINE_MODE=true
VITE_CACHE_DURATION=86400