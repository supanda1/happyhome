{
  "name": "household-services-backend",
  "version": "1.0.0",
  "description": "Backend API for Household Services Application",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "db:migrate": "npm run build && node dist/scripts/migrate.js",
    "db:seed": "npm run build && node dist/scripts/seed.js",
    "db:setup": "npm run db:migrate && npm run db:seed"
  },
  "keywords": [
    "household-services",
    "api",
    "postgresql",
    "express"
  ],
  "author": "Happy Homes",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.2.1",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "pg": "^8.11.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.7",
    "@types/node": "^20.8.6",
    "@types/pg": "^8.10.7",
    "@types/uuid": "^9.0.6",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
